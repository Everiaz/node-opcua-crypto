{
  "name": "node-opcua-crypto",
  "version": "1.0.2",
  "description": "Crypto tools for Node-OPCUA",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "npx tsc",
    "prepare": "npm run build",
    "lint": "npx tslint lib/*.ts test/*.ts",
    "test": "npx mocha test -r ts-node/register -r source-map-support/register -R spec --recursive --timeout 200000 --bail test/**/*.ts",
    "makedoc": "npx typedoc -out doc",
    "cost-of-modules": "npx cost-of-modules --no-install",
    "release-it": "npx release-it"
  },
  "keywords": [
    "OPCUA",
    "opcua",
    "m2m",
    "iot",
    "opc ua",
    "internet of things"
  ],
  "author": "Etienne Rossignon",
  "license": "MIT",
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/should": "^13.0.0",
    "@types/node": "^10.12.0",
    "@types/underscore": "^1.8.9",
    "lorem-ipsum": "^1.0.6",
    "mocha": "^5.2.0",
    "should": "^13.2.3",
    "source-map-support": "^0.5.9",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.3"
  },
  "dependencies": {
    "better-assert": "^1.0.2",
    "chalk": "^2.4.1",
    "hexy": "^0.2.11",
    "jsrsasign": "^8.0.12",
    "ssh-key-to-pem": "^0.11.1",
    "underscore": "^1.9.1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/node-opcua/node-opcua-crypto.git"
  },
  "bugs": {
    "url": "https://github.com/node-opcua/node-opcua-crypto/issues"
  }
}
